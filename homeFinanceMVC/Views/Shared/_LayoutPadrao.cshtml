<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

    <script src="https://code.highcharts.com/highcharts.js"></script>
    <script src="https://code.highcharts.com/modules/exporting.js"></script>

    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <script src="~/Scripts/materialize.min.js"></script>
    <script src="~/Scripts/jquery-2.1.1.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>

    <link href="~/Content/bootstrap.cosmo.min.css" rel="stylesheet" />
    <link href="~/Content/estilo.css" rel="stylesheet" />
    <link href="~/Content/materialize.min.css" rel="stylesheet" />

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>

    <header>
        <!-- Aqui empieza la pagina maestra -->

        <ul id="slide-out" class="side-nav fixed z-depth-2">
            <li class="center no-padding">
                <div class="indigo darken-2 white-text" style="height: 180px;">
                    <div class="row">
                        <img style="margin-top: 5%;" width="100" height="100" src="~/imagenes/homefinance2.png" class=" responsive-img" />
                        <p style="margin-top: -5%;">
                            <asp:Label ID="lbUsu" runat="server" Text="" style="margin-left: 20%;"></asp:Label>

                            <!--  Tirth Patel -->

                            @Session["nombreusu"]


                        </p>
                    </div>
                </div>
            </li>


            <ul class="collapsible" data-collapsible="accordion">
                <li id="dash_users">
                    <div id="dash_users_header" class="collapsible-header waves-effect"><b>Usuarios</b></div>
                    <div id="dash_users_body" class="collapsible-body">
                        <ul>
                            <li id="users_seller">
                                <a class="waves-effect" style="text-decoration: none;" href="#!">Insertar</a>
                            </li>

                            <li id="users_customer">
                                <a class="waves-effect" style="text-decoration: none;" href="#!">Manipular Datos</a>
                            </li>
                        </ul>
                    </div>
                </li>

                <li id="dash_products">
                    <div id="dash_products_header" class="collapsible-header waves-effect"><b>Cuentas</b></div>
                    <div id="dash_products_body" class="collapsible-body">
                        <ul>
                            <li id="products_product">
                                <a class="waves-effect" style="text-decoration: none;" href="#!">Insertar</a>
                                <a class="waves-effect" style="text-decoration: none;" href="#!">Manipular Datos</a>
                            </li>
                        </ul>
                    </div>
                </li>

                <li id="dash_categories">
                    <div id="dash_categories_header" class="collapsible-header waves-effect"><b>Eventos</b></div>
                    <div id="dash_categories_body" class="collapsible-body">
                        <ul>
                            <li id="categories_category">
                                <a class="waves-effect" style="text-decoration: none;" href="#!">Insertar</a>
                            </li>

                            <li id="categories_sub_category">
                                <a class="waves-effect" style="text-decoration: none;" href="#!">Manipular Datos</a>
                            </li>
                        </ul>
                    </div>
                </li>

                <li id="dash_brands">
                    <div id="dash_brands_header" class="collapsible-header waves-effect"><b>Movimiento</b></div>
                    <div id="dash_brands_body" class="collapsible-body">
                        <ul>
                            <li id="brands_brand">
                                <a class="waves-effect" style="text-decoration: none;" href="#!">Lanzar</a>
                            </li>

                            <li id="brands_sub_brand">
                                <a class="waves-effect" style="text-decoration: none;" href="#!">Manipular Datos</a>
                            </li>
                        </ul>
                    </div>
                </li>

                <li id="dash_brands">
                    <div id="dash_brands_header" class="collapsible-header waves-effect"><b>@Html.ActionLink("Graficos", "Grafico", "Home")</b></div>
                </li>

                <li id="dash_brands">
                    <div id="dash_brands_header" class="collapsible-header waves-effect"><b>Graficos</b></div>
                    <div id="dash_brands_body" class="collapsible-body">
                        <ul>
                            <li id="brands_brand">
                                <a class="waves-effect" style="text-decoration: none;" href="#!">Grafico de Salidas</a>
                            </li>

                            <li id="brands_sub_brand">
                                <a class="waves-effect" style="text-decoration: none;" href="#!">Grafico de Entradas</a>
                            </li>
                        </ul>
                    </div>
                </li>

            </ul>
        </ul>

        <ul class="dropdown-content" id="user_dropdown">

            <li><a class="indigo-text" href="#!">Perfil</a></li>
            <li>@Html.ActionLink("Logout", "Logout", "Login")</li>
        </ul>

        <nav class="indigo" role="navigation">
            <div class="nav-wrapper">
                <!--   <a data-activates="slide-out" class="button-collapse show-on-large" href="#!"><img style="margin-top: 5px; margin-left: 5px; width: auto;" src="imagenes/menu.png" class="auto-style1" /></a> -->

                <ul class="right hide-on-med-and-down">
                    <li>
                        <a class='right dropdown-button' href='' data-activates='user_dropdown'><i class=' material-icons'>account_circle</i></a>
                    </li>
                </ul>

                <a href="#" data-activates="slide-out" class="button-collapse"><i class="mdi-navigation-menu"></i></a>
            </div>
        </nav>

        <nav>
            <!-- Opciones en el menu -->
            <div class="nav-wrapper indigo darken-2">
                <a style="margin-left: 20px" class="breadcrumb" href="#!">Admin</a>
                <a class="breadcrumb" href="#!">Index</a>

                <div style="margin-right: 20px;" id="timestamp" class="right"></div>
            </div>
        </nav>
        <!-- Menu Flotante - Pulsando-->
        <div class="fixed-action-btn click-to-toggle" style="bottom: 45px; right: 24px;">
            <a class="btn btn-floating btn-large cyan pulse">
                <i class="large material-icons">add</i>
            </a>

            <ul>
                <li>
                    <a class="btn-floating red"><i class="material-icons">note_add</i></a>
                <li class="btn-floating fab-tip">@Html.ActionLink("Logout", "Logout", "Login")</li>

                <li>
                    <a class="btn-floating yellow darken-1"><i class="material-icons">add_a_photo</i></a>
                    <a href="InsertaEventos.aspx?recorrido=10" class="btn-floating fab-tip">Insertar Eventos</a>
                </li>

                <li>
                    <a class="btn-floating green"><i class="material-icons">alarm_add</i></a>
                    <a href="MisEventos.aspx" class="btn-floating fab-tip">Mis Eventos</a>
                </li>

                <li>
                    <a class="btn-floating blue"><i class="material-icons">monetization_on</i></a>
                    <a href="/LanzaMovimiento.aspx?recorrido=10" class="btn-floating fab-tip">Lanzar Evento</a>
                </li>
            </ul>
        </div>


    </header>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Mi aplicación ASP.NET</p>
        </footer>
    </div>

    <script type="text/javascript" src="https://code.jquery.com/jquery-2.2.4.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.6/js/materialize.min.js"></script>


    <script type="text/javascript">
        $(document).ready(function () {
            $('.button-collapse').sideNav();
            $('.collapsible').collapsible();
            $('select').material_select();

        });

    </script>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)


</body>
</html>
